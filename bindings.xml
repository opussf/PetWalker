<Bindings>
	<Binding name="AUTO" header="THISADDON" category="ADDONS">
        PetWalker:AutoToggle()
    </Binding>
    <Binding name="MANUAL" category="ADDONS">
        PetWalker:NewPet(C_PetJournal.GetSummonedPetGUID())
    </Binding>
    <Binding name="DISMISS" category="ADDONS">
        PetWalker:DismissAndDisable()
    </Binding>
</Bindings>
